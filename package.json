{
  "name": "vivovi",
  "version": "0.1.0",
  "description": "A Next.js Blog project using Supabase",
  "private": true,
  "type": "module",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:clean": "rm -rf .next && rm -rf .velite && next build",
    "start": "next start",
    "lint": "next lint && pnpm run prisma:validate",
    "lint:fix": "next lint --fix",
    "format": "pnpm run prettier:check",
    "format:fix": "pnpm run prettier:write && pnpm run prisma:format",
    "type-check": "pnpm exec tsc --noEmit",
    "prettier:check": "pnpm exec prettier --check 'src/**/*.{ts,tsx,js,jsx,json}' --cache",
    "prettier:write": "pnpm exec prettier --write 'src/**/*.{ts,tsx,js,jsx,json}' --cache",
    "supabase:start": "pnpm supabase start",
    "supabase:stop": "pnpm supabase stop",
    "prisma:dev": "dotenv -e .env.local -- pnpm exec prisma migrate dev",
    "prisma:status": "dotenv -e .env.local -- pnpm exec prisma migrate status",
    "prisma:reset": "dotenv -e .env.local -- pnpm exec prisma migrate reset",
    "prisma:generate": "dotenv -e .env.local -- pnpm exec prisma generate",
    "prisma:push": "dotenv -e .env.local -- pnpm exec prisma db push",
    "prisma:pull": "dotenv -e .env.local -- pnpm exec prisma db pull",
    "prisma:studio": "dotenv -e .env.local -- pnpm exec prisma studio",
    "prisma:format": "pnpm exec prisma format",
    "prisma:validate": "dotenv -e .env.local -- pnpm exec prisma validate",
    "prepare": "husky",
    "db:seed": "dotenv -e .env.local -- prisma db seed",
    "db:seed:prod": "prisma db seed"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,cjs,mjs}": [
      "pnpm run lint:fix --dir src",
      "pnpm run format:fix"
    ],
    "*.{css,md,json,yml,yaml}": [
      "pnpm run format:fix"
    ],
    "**/*.prisma": [
      "pnpm run prisma:validate",
      "pnpm run prisma:format"
    ]
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@codesandbox/sandpack-react": "^2.20.0",
    "@giscus/react": "^3.1.0",
    "@icons-pack/react-simple-icons": "^13.8.0",
    "@next/third-parties": "^15.5.6",
    "@prisma/client": "^6.17.1",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/supabase-js": "^2.75.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "es-toolkit": "^1.40.0",
    "lucide-react": "^0.546.0",
    "metascraper": "^5.49.5",
    "metascraper-description": "^5.49.5",
    "metascraper-image": "^5.49.5",
    "metascraper-logo-favicon": "^5.49.5",
    "metascraper-publisher": "^5.49.5",
    "metascraper-title": "^5.49.5",
    "motion": "^12.23.24",
    "next": "15.5.6",
    "next-themes": "^0.4.6",
    "nuqs": "^2.7.2",
    "react": "^19.2.0",
    "react-children-utilities": "^2.10.0",
    "react-dom": "^19.2.0",
    "react-fast-marquee": "^1.6.5",
    "react-tweet": "^3.2.2",
    "server-only": "^0.0.1",
    "sharp": "^0.34.4"
  },
  "devDependencies": {
    "@jsdevtools/rehype-toc": "^3.0.2",
    "@shikijs/transformers": "^3.13.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^24.8.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.38.0",
    "eslint-config-next": "15.5.6",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.4",
    "node-loader": "^2.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "prisma": "^6.17.1",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-pretty-code": "^0.14.1",
    "rehype-semantic-blockquotes": "^3.0.7",
    "rehype-slug": "^6.0.0",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "remark-heading-id": "^1.0.1",
    "shiki": "^3.13.0",
    "supabase": "^2.51.0",
    "tailwind-merge": "^3.3.1",
    "tailwind-scrollbar": "^4.0.2",
    "tailwindcss": "^4.1.14",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "velite": "^0.3.0",
    "yakuhanjp": "^4.1.1"
  }
}
